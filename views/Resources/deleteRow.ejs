<%- include ('../parts/header')%>
            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid px-4">
                        <%- messages('messages',locals)%> 

                        <% if (errors) {%>
                            <% errors.forEach(function(error) { %>
                                <div class="alert alert-danger">
                                    <%= error.msg %>
                                </div>
                            <% });%>
                        <% }%>

                       
                        <h1 class="text-center mt-4">Eliminar Fila</h1>


                        <form method="POST" class="mt-3" action="/resources/deleteRow/<%= id_Presupuesto %>">

                            


                            <div class="form-group mt-3">
                                <label for="" class="fw-bold">Concepto</label>
                                <select name="concepto" class="form-control">
                                    <% instanceRowEntry.forEach(function(concept){ %>
                                        <option value="<%= concept.id_fila %>"><%= concept.nombre %></option>
                                    <% }); %>
                                </select>
                            </div>
                    
                           
                    
                           
                            <button type="submit" class="btn btn-primary mt-3">Eliminar</button>
                          </form>
                        
                       
                    </div>
                </main>
               
            </div>
        </div>
       
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
        <script src="/js/scripts.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/simple-datatables@latest" crossorigin="anonymous"></script>
        <script src="/js/datatables-simple-demo.js"></script>


        <script src="/libs/js/jquery.datetimepicker.full.min.js"></script> 
        <script type="text/javascript">
            $(function() {
                $('#datetimepicker').datetimepicker({
                    timepicker:false,//para quitar la hora
 format:'Y-m-d'
   
});
            });
        </script>




    </body>
    


</html>
